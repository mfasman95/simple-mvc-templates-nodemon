{
  "name": "simple-mvc-no-templates",
  "version": "1.0.0",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "description": "A simple Express MVC using handlebar templates",
  "main": "./server/app.js",
  "scripts": {
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./src --fix",
    "test": "echo \"tests complete\""
  },
  "author": "Cody Van De Mark",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "serve-favicon": "^2.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "rules": {
      "no-underscore-dangle": "off",
      "no-plusplus": "off"
    }
  },
  "babel": {
    "presets": ["env"]
